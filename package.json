{
  "name": "@gigerit/vuetify-date-input-advanced",
  "version": "0.1.0",
  "description": "Advanced date input for Vuetify 4",
  "type": "module",
  "main": "./dist/vuetify-date-input-advanced.umd.js",
  "module": "./dist/vuetify-date-input-advanced.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vuetify-date-input-advanced.es.js",
      "require": "./dist/vuetify-date-input-advanced.umd.js"
    },
    "./styles": "./dist/vuetify-date-input-advanced.css",
    "./styles/variables": "./src/styles/variables.scss"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "dev": "vite --config playground/vite.config.ts",
    "dev:lib": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "vuetify": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/node": "^22.13.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@typescript-eslint/parser": "^8.25.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.20.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "sass": "^1.85.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
  }
}
