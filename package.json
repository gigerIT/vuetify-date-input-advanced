{
  "name": "vuetify-date-input-advanced",
  "version": "1.0.0",
  "description": "Advanced date picker/input for Vuetify 3 with multi-month display, date ranges, presets, swipe navigation, and fullscreen mobile support",
  "type": "module",
  "main": "./dist/vuetify-date-input-advanced.umd.js",
  "module": "./dist/vuetify-date-input-advanced.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vuetify-date-input-advanced.es.js",
      "require": "./dist/vuetify-date-input-advanced.umd.js"
    },
    "./styles": "./dist/vuetify-date-input-advanced.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.sass",
    "*.scss"
  ],
  "scripts": {
    "dev": "vite --config playground/vite.config.ts",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build:lib": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ --ext .ts,.vue",
    "lint:fix": "eslint src/ --ext .ts,.vue --fix"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "vuetify": "^3.11.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/test-utils": "^2.4.6",
    "sass": "^1.97.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vue": "^3.5.28",
    "vue-tsc": "^3.2.4",
    "vuetify": "^3.11.8"
  },
  "license": "MIT",
  "keywords": [
    "vuetify",
    "date-picker",
    "date-range",
    "vue3",
    "vuetify3",
    "date-input",
    "multi-month",
    "presets"
  ]
}
